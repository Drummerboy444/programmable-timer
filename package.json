{
  "name": "programmable-timer",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start --dev-client",
    "lint": "eslint --ext .ts,.tsx src --max-warnings=0",
    "check-types": "tsc",
    "test": "jest",
    "build:development:android": "eas build -p android --profile development",
    "build:development:ios-simulator": "eas build -p ios --profile development-ios-simulator",
    "build:production:android": "eas build -p android --profile production",
    "build:production:ios": "eas build -p ios --profile production",
    "prepare": "husky install"
  },
  "dependencies": {
    "expo": "~48.0.9",
    "expo-dev-client": "~2.1.6",
    "expo-status-bar": "~1.4.4",
    "fp-ts": "^2.13.1",
    "react": "18.2.0",
    "react-native": "0.71.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/jest": "^29.5.0",
    "@types/react": "~18.0.14",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "eas-cli": "3.8.1",
    "eslint": "^8.36.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest": "^29.2.1",
    "jest-expo": "^48.0.2",
    "prettier": "^2.8.6",
    "typescript": "^4.9.4"
  },
  "private": true,
  "jest": {
    "preset": "jest-expo"
  }
}
